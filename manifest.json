{
  "manifest_version": 2,
  "name": "Nord Theme By Ryan The Ghost",
  "version": "0.1",
  "author": "Ryan The Ghost",
  "permissions": [],
  "browser_action": {
  "default_icon": "logo.png"
  },
  "content_scripts": [{
      "matches": [
        "*://discord.com/*",
        "*://www.youtube.com/*",
        "*://www.deviantart.com/*",
        "*://monkeytype.com/*",
        "*://www.reddit.com/*"
      ],
      "js": ["javascript/urlCheck.js"]
    }],
  "web_accessible_resources": [
    "css/discord.css",
    "css/youtube.css",
    "css/deviantart.css",
    "css/monkeytype.css",
    "css/reddit.css"
  ]
  
}
